    <Scene>
        <!-- Center: at the middle of the room, but on the floor -->
        <!-- Size: width = 807cm | heigth = 360 (height of square) | depth = 1168cm -->
        
        <Group DEF='WallGroup'>
            <!-- Front wall (with main door) -->
            <!-- Translation:
                    x = 0cm
                    y = 0.5*360cm (own height) = 180cm
                    z = 0.5*1168cm (room's depth) - 0.5*24cm (own depth) = 572cm -->
            <Transform DEF='FrontWallTransform' translation='0 1.80 5.72'>
                <Shape DEF='FrontWallShape'>
                    <Appearance DEF='WallAppearance'>
                        <Material diffuseColor='1 1 1'/>
                    </Appearance>
                    <!-- Size:
                            w = 807cm (width of the room)
                            h = 360cm (height of the room)
                            d = 24cm (depth of the wall) -->
                    <Box size='8.07 3.60 0.24'/>
                </Shape>
            </Transform>
            <!-- Back wall (with window) -->
            <Transform DEF='BackWallTransform' translation='0 1.80 -5.72'>
                <Shape DEF='BackWallShape'>
                    <Appearance USE='WallAppearance'/>
                    <Box size='8.07 3.60 0.24'/>
                </Shape>
            </Transform>
            <!-- Side walls (perspective: inside out; in the door direction) -->
            <!-- Translation:
                    x = 0.5*807cm (room's width) - 0.5*24cm (width of wall) = 391.5cm
                    y = 0.5*360cm (height of the wall) = 180cm
                    z = 0cm -->
            <Transform DEF='LeftSideWallTransform' translation='3.915 1.80 0'>
                <Shape DEF='SideWallShape'>
                    <Appearance USE='WallAppearance'/>
                    <!-- Size:
                            w = 24cm (width/depth of wall)
                            h = 360cm (height of room)
                            d = 1168cm (depth of room) - 2*24cm (depth of fron/back wall) = 1120cm -->
                    <Box size='0.24 3.60 11.52'/>
                </Shape>
            </Transform>
            <Transform DEF='RightSideWallTransform' translation='-3.915 1.80 0'>
                <Shape USE='SideWallShape'/>
            </Transform>
        </Group>
    </Scene>
