    <Scene>
        <!-- Center: at the 'center' of the room, but on the floor (y = 0) -->
        <!-- Size: width = 755cm | height = 360cm (height of square) | depth = 390cm (main hall) -->
        
        <Group DEF='WallGroup'>
            <!-- Fron wall (with main door) -->
            <!-- Translation:
                    x = 0cm
                    y = 0.5*360cm (own height) = 180cm
                    z = 0.5*390cm (room's depth) - 0.5*24cm (own depth) = 183cm -->
            <Transform DEF='FrontWallTransform' translation='0 1.80 1.83'>
                <Shape DEF='FrontWallShape'>
                    <Appearance DEF='WallAppearance'>
                        <Material diffuseColor='1 1 1'/>
                    </Appearance>
                    <!-- Size:
                            w = 755cm (room's width)
                            h = 360cm (room's height)
                            d = 24cm (depth of wall) -->
                    <Box size='7.55 3.60 0.24'/>
                </Shape>
            </Transform>
            <!-- Back wall (with window) -->
            <Transform DEF='BackWallTransform' translation='0 1.80 -1.83'>
                <Shape DEF='BackWallShape'>
                    <Appearance USE='WallAppearance'/>
                    <Box size='7.55 3.60 0.24'/>
                </Shape>
            </Transform>
            <!-- Side walls (perspective: inside looking out into the door direction) -->
            <!-- Translation:
                    x = 0.5*755cm (room's width) - 0.5*24cm (own width) = 365.5cm
                    y = 0.5*360cm (own height) = 180cm
                    z = 0cm -->
            <Transform DEF='LeftSideWallTransform' translation='3.655 1.80 0'>
                <Shape DEF='SideWallShape'>
                    <Appearance USE='WallAppearance'/>
                    <!-- Size:
                            w = 24cm (= depth of front/back wall)
                            h = 360cm (room's height)
                            d = 390cm (room's depth) - 2*24cm (depth of front & back walls) = 342cm -->
                    <Box size='0.24 3.60 3.42'/>
                </Shape>
            </Transform>
            <Transform DEF='RightSideWallTransform' translation='-3.655 1.80 0'>
                <Shape USE='SideWallShape'/>
            </Transform>
        </Group>
    </Scene>
