    <Scene>
        <!-- Center: at the middle of the room, but on the floor -->
        <!-- Size: width = 190cm | heigth = 320 (height of square) | depth = 390cm (main hall) -->
        
        <Group DEF='WallGroup'>
            <!-- Front wall (with main door) -->
            <!-- Translation:
                    |y| = 0.5*320cm = 160cm
                    |z| = 0.5*390cm - 0.5*10cm  = 190cm -->
            <Transform DEF='FrontWallTransform' translation='0 1.6 1.9'>
                <Shape DEF='FrontWallShape'>
                    <Appearance DEF='WallAppearance'>
                        <Material diffuseColor='1 1 1'/>
                    </Appearance>
                    <!-- Depth = 10cm -->
                    <Box size='1.90 3.20 0.1'/>
                </Shape>
            </Transform>
            <!-- Back wall (with window) -->
            <Transform DEF='BackWallTransform' translation='0 1.6 -1.9'>
                <Shape DEF='BackWallShape'>
                    <Appearance USE='WallAppearance'/>
                    <Box size='1.90 3.20 0.10'/>
                </Shape>
            </Transform>
            <!-- Side walls (perspective: inside out; in the door direction) -->
            <!-- Translation:
                    |x| = 0.5*190cm (room's width) - 0.5*10cm = 90cm -->
            <Transform DEF='LeftSideWallTransform' translation='0.9 1.6 0'>
                <Shape DEF='SideWallShape'>
                    <Appearance USE='WallAppearance'/>
                    <!-- Size:
                        width = 10cm (depth of fron/back wall)
                        depth = 390cm (depth of room) - 2*10cm (depth of fron/back wall) = 370cm -->
                    <Box size='0.10 3.20 3.70'/>
                </Shape>
            </Transform>
            <Transform DEF='RightSideWallTransform' translation='-0.9 1.6 0'>
                <Shape USE='SideWallShape'/>
            </Transform>
        </Group>
    </Scene>
</X3D>