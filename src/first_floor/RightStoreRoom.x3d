    <Scene>
        <!-- 'Store room' - Right next to the Elevators -->
        <!-- Center: at the 'center' of the room, but on the floor (y = 0) -->
        <!-- Size: width = 205cm | height = 320cm (height of square) | depth = 360cm (slightly 'less' than at the center corridor) -->
        
        <Group DEF='WallGroup'>
            <!-- Fron wall (with main door) -->
            <!-- Translation:
                    |y| = 0.5*320cm (height of the room) = 160cm
                    |z| = 0.5*360cm (depth of the room) - 0.5*24cm (depth of front wall) = 168cm -->
            <Transform DEF='FrontWallTransform' translation='0 1.6 1.68'>
                <Shape DEF='FrontWallShape'>
                    <Appearance DEF='WallAppearance'>
                        <Material diffuseColor='1 1 1'/>
                    </Appearance>
                    <!-- Depth = 24cm -->
                    <Box size='2.05 3.20 0.24'/>
                </Shape>
            </Transform>
            <!-- Back wall (with window) -->
            <Transform DEF='BackWallTransform' translation='0 1.6 -1.68'>
                <Shape DEF='BackWallShape'>
                    <Appearance USE='WallAppearance'/>
                    <Box size='2.05 3.20 0.24'/>
                </Shape>
            </Transform>
            <!-- Side walls (perspective: inside looking out into the door direction) -->
            <!-- Translation:
                    |x| = 0.5*205cm (width of the room) - 0.5*24cm (depth of side wall) = 90.5cm
                    |y| = 0.5*320cm (height of the room) = 160cm -->
            <Transform DEF='LeftSideWallTransform' translation='0.905 1.6 0'>
                <Shape DEF='SideWallShape'>
                    <Appearance USE='WallAppearance'/>
                    <!-- Size:
                        width = 24cm (depth of fron/back wall)
                        depth = 360cm (depth of room) - 2*24cm (depth of fron/back wall) = 312cm -->
                    <Box size='0.24 3.20 3.12'/>
                </Shape>
            </Transform>
            <Transform DEF='RightSideWallTransform' translation='-0.905 1.6 0'>
                <Shape USE='SideWallShape'/>
            </Transform>
        </Group>
    </Scene>
</X3D>